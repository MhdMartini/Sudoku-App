<GridWidget>:
    canvas:
        Color:
            rgba: 0, 0.1, 0.2, 1  # Dark blue

        # Vertical Lines
        Line:
            points: self.width * 0.02 + self.width * 0/9, self.height * 1/2, self.width * 0.02 + self.width * 0/9, self.height * 3/2
            width : 2
        Line:
            points: self.width * 0.02 + self.width * 1/9, self.height * 1/2, self.width * 0.02 + self.width * 1/9, self.height * 3/2
            width : 1
        Line:
            points: self.width * 0.02 + self.width * 2/9, self.height * 1/2, self.width * 0.02 + self.width * 2/9, self.height * 3/2
            width : 1

        Line:
            points: self.width * 0.02 + self.width * 3/9, 0 + self.height * 1/2, self.width * 0.02 + self.width * 3/9, self.height * 3/2
            width : 2
        Line:
            points: self.width * 0.02 + self.width * 4/9, self.height * 1/2, self.width * 0.02 + self.width * 4/9, self.height * 3/2
            width : 1
        Line:
            points: self.width * 0.02 + self.width * 5/9, self.height * 1/2, self.width * 0.02 + self.width * 5/9, self.height * 3/2
            width : 1

        Line:
            points: self.width * 0.02 + self.width * 6/9, 0 + self.height * 1/2, self.width * 0.02 + self.width * 6/9, self.height * 3/2
            width : 2
        Line:
            points: self.width * 0.02 + self.width * 7/9, self.height * 1/2, self.width * 0.02 + self.width * 7/9, self.height * 3/2
            width : 1
        Line:
            points: self.width * 0.02 + self.width * 8/9, self.height * 1/2, self.width * 0.02 + self.width * 8/9, self.height * 3/2
            width : 1

        Line:
            points: self.width * 0.02 + self.width * 9/9, self.height * 1/2, self.width * 0.02 + self.width * 9/9, self.height * 3/2
            width : 2


        # Horizontal Lines:
        Line:
            points: self.width * 0.02, self.height * 1/2, self.width + self.width * 0.02, self.height * 1/2
            width : 2
        Line:
            points: self.width * 0.02, self.height * 1/2 + self.height * 1/9, self.width + self.width * 0.02, self.height * 1/2 + self.height * 1/9
            width : 1
        Line:
            points: self.width * 0.02, self.height * 1/2 + self.height * 2/9, self.width + self.width * 0.02, self.height * 1/2 + self.height * 2/9
            width : 1

        Line:
            points: self.width * 0.02, self.height * 1/2 + self.height * 3/9, self.width + self.width * 0.02, self.height * 1/2 + self.height * 3/9
            width : 2
        Line:
            points: self.width * 0.02, self.height * 1/2 + self.height * 4/9, self.width + self.width * 0.02, self.height * 1/2 + self.height * 4/9
            width : 1
        Line:
            points: self.width * 0.02, self.height * 1/2 + self.height * 5/9, self.width + self.width * 0.02, self.height * 1/2 + self.height * 5/9
            width : 1

        Line:
            points: self.width * 0.02, self.height * 1/2 + self.height * 6/9, self.width + self.width * 0.02, self.height * 1/2 + self.height * 6/9
            width : 2
        Line:
            points: self.width * 0.02, self.height * 1/2 + self.height * 7/9, self.width + self.width * 0.02, self.height * 1/2 + self.height * 7/9
            width : 1
        Line:
            points: self.width * 0.02, self.height * 1/2 + self.height * 8/9, self.width + self.width * 0.02, self.height * 1/2 + self.height * 8/9
            width : 1

        Line:
            points: self.width * 0.02, self.height * 1/2 + self.height * 9/9, self.width + self.width * 0.02, self.height * 1/2 + self.height * 9/9
            width : 2